/* pages/tablehistory/tablehistorydetail/tablehistorydetail.wxss */
.item-box{
  width: 700rpx;
  margin: 0 auto;
  padding:40rpx 0;
  font-size: 30rpx;
}

.items{
  width: 100%;
}

.item{
  position: relative;
  border-top: 2rpx solid rgb(248, 244, 244);
  border-bottom: 2rpx solid rgb(248, 244, 244);
  height: 106rpx;
  line-height: 120rpx;
  overflow: hidden;
}

.inner.txt{
  background-color: #fff;
  width: 100%;
  z-index: 5;
  transition: left 0.2s ease-in-out;
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis;
}

.item-data-left{
width: 68%;
display: inline-block;
background: #eee;
text-align: left;
}

.item-data-right{
 display: inline-block;
 width: 32%;
  text-align: center;
}

.anspace{
color: #fa7e04;
}

.time {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  text-align: center;
  font-size: 30rpx;
  margin: 50rpx 50rpx 50rpx;
  color: #9B9B9B;
}